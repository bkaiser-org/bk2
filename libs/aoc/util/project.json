{
  "name": "aoc-util",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/aoc/util/src",
  "prefix": "bk",
  "projectType": "library",
  "tags": ["type:util", "scope:aoc", "platform:mobile", "platform:web"],
  "targets": {
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write ."
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --check ."
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/aoc/util",
        "main": "libs/aoc/util/src/index.ts",
        "tsConfig": "libs/aoc/util/tsconfig.lib.json",
        "assets": ["libs/aoc/util/*.md"],
        "updateBuildableProjectDepsInPackageJson": false
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "configFile": "libs/aoc/util/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}

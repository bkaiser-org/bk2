{
  "name": "task/ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/task/ui/src",
  "prefix": "bk",
  "projectType": "library",
  "tags": ["type:ui", "scope:task", "platform:mobile", "platform:web"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/task/ui",
        "main": "libs/task/ui/src/index.ts",
        "tsConfig": "libs/task/ui/tsconfig.lib.json",
        "assets": ["libs/task/ui/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/task/ui/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}

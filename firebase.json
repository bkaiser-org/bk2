{
  "firestore": {
    "database": "(default)",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "apphosting": {
    "backendId": "bko-euw4-backend",
    "rootDir": "apps/test-app",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions",
      "../../dist"
    ],
    "environmentVariables": {
      "GOOGLE_NODE_RUN_SCRIPTS": "",
      "NODE_VERSION": "23.8.0"
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "apps/test-app"
    },
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 5000
    },
    "singleProjectMode": true
  }
}
